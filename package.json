{
	"name": "gitignore",
	"displayName": "gitignore",
	"description": "An extension that lets you pull .gitignore files from the https://github.com/github/gitignore repository",
	"version": "0.1.4",
	"publisher": "codezombiech",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/CodeZombieCH/vscode-gitignore"
	},
	"bugs": {
		"url": "https://github.com/CodeZombieCH/vscode-gitignore/issues"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^0.10.10"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:addgitignore"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "addgitignore",
				"title": "Add gitignore"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^1.8.5",
		"vsce": "^1.2.3",
		"vscode": "^0.11.0"
	},
	"dependencies": {
		"github": "^0.2.4"
	}
}